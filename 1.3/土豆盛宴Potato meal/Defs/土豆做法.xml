<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="LingPTTMealBase" Abstract="True">
    <thingClass>ThingWithComps</thingClass>
    <category>Item</category>
    <useHitPoints>true</useHitPoints>
    <selectable>true</selectable>
    <graphicData>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>600</WorkToMake>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>1.0</Flammability>
	  <Beauty>0</Beauty>
	  <Mass>0.3</Mass>
	  <DeteriorationRate>10</DeteriorationRate>
	 </statBases>
    <altitudeLayer>Item</altitudeLayer>
    <stackLimit>75</stackLimit>
    <tickerType>Rare</tickerType>
    <socialPropernessMatters>true</socialPropernessMatters>
    <thingCategories>
      <li>FoodMeals</li>
    </thingCategories>
    <alwaysHaulable>true</alwaysHaulable>
    <comps>
      <li Class="CompProperties_Forbiddable" />
      <li Class="CompProperties_Rottable">
        <daysToRotStart>4</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <pathCost>15</pathCost>
    <resourceReadoutPriority>Last</resourceReadoutPriority>
    <drawGUIOverlay>true</drawGUIOverlay>
    <uiIconForStackCount>1</uiIconForStackCount>
    <ingestible>
      <foodType>Meal</foodType>
      <optimalityOffsetHumanlikes>5</optimalityOffsetHumanlikes>
      <optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
	    <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
      <preferability>MealFine</preferability>
    </ingestible>
  </ThingDef>
  <RecipeDef Name="LingCookMealBase" Abstract="True">
    <workSpeedStat>CookSpeed</workSpeedStat>
    <requiredGiverWorkType>Cooking</requiredGiverWorkType>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <!--<researchPrerequisite>ZeroTechMeals</researchPrerequisite>-->
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
	 <recipeUsers>
	  <li>FueledStove</li>
	  <li>ElectricStove</li>
	 </recipeUsers>
	 <fixedIngredientFilter>
	  <categories>
		<li>FoodRaw</li>
	  </categories>
	  <specialFiltersToDisallow>
		<li>AllowPlantFood</li>
	  </specialFiltersToDisallow>
	 </fixedIngredientFilter>
	 <defaultIngredientFilter>
	  <categories>
		<li>FoodRaw</li>
	  </categories>
	  <disallowedCategories>
		<li>EggsFertilized</li>
	  </disallowedCategories>
	  <disallowedThingDefs>
		<li>Meat_Human</li>
		<li>Meat_Megaspider</li>
		<li>InsectJelly</li>
	  </disallowedThingDefs>
	 </defaultIngredientFilter>
	 <workSkill>Cooking</workSkill>
  </RecipeDef>

  <!-- 土豆丝 -->
  <ThingDef ParentName="LingPTTMealBase">
    <defName>LPTT_ShreddedPotatoes</defName>
    <label>土豆丝</label>
    <description>将土豆清洗切丝后,配以简单的配料,经过清炒,便是一道爽口的菜品</description>
    <graphicData>
      <texPath>Things/Item/Meal/TuDouSi</texPath>
    </graphicData>
    <statBases>
      <Nutrition>0.4</Nutrition>
    </statBases>
    <ingestible>
      <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>LPTT_ShreddedPotatoes</hediffDef>
          <severity>0.4</severity>
        </li>
      </outcomeDoers>
    </ingestible>
  </ThingDef>
  <RecipeDef ParentName="LingCookMealBase">
    <defName>LPTT_ShreddedPotatoes</defName>
    <label>炒土豆丝</label>
    <description>炒土豆丝.</description>
    <jobString>炒土豆丝.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawPotatoes</li>
          </thingDefs>
        </filter>
        <count>0.5</count>
      </li>
    </ingredients>
	  <products>
		  <LPTT_ShreddedPotatoes>2</LPTT_ShreddedPotatoes>
	  </products>
  </RecipeDef>
  <HediffDef>
    <defName>LPTT_ShreddedPotatoes</defName>
	  <description>你品尝了一碗土豆丝,这使你感到希望的存在.</description>
    <hediffClass>HediffWithComps</hediffClass>
    <label>土豆丝</label>
    <defaultLabelColor>(1,1,1)</defaultLabelColor>
    <scenarioCanAdd>true</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <isBad>false</isBad>
    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-1</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Manipulation</capacity>
            <offset>0.05</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>

